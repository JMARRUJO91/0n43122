            i = !0
        } catch (t) {
            try {
                (r = new Error(n)).stack.length > 0 && (console.groupCollapsed($$WP.Debug.messagePrefix + n),
                console.error(r.stack, ""),
                console.groupEnd(),
                i = !0)
            } catch (n) {}
        }
        if (!i)
            try {
                console.error($$WP.Debug.messagePrefix + n, "")
            } catch (n) {}
    } else
        try {
            console.error($$WP.Debug.messagePrefix + n, "")
        } catch (n) {}
    $$WP.Debug._errorCount++;
    $$WP.Debug._updateErrorCountDisplay()
}
