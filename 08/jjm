    o = 0,
    "main" !== h.safeAttr("id") && "sidebar" !== h.safeAttr("id") || (i.top += $afe.select("#header").outerHeight(),
    (a = $afe.select("#footer").outerHeight(!1)) > 0 && (i.height -= a,
    i.bottom = i.top + i.height)),
    r = n.boxModel(!1),
