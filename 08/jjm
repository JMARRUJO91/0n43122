    t.outerWidth(i.width - 16 - s),
    l && (u = t.boxModel(!1),
    e = r.top - u.height),
    $$WP.Debug.log("Popup cannot be fit to screen"))) : ($$WP.Debug.log("Popup cannot be fit to screen"),
    n.addClass("onRight"),
    e = r.top,
    f = r.right),
    {
        top: e,
        left: f
    }
