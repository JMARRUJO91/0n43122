$$WPUtil.printElement = function(n) {
    var i, t;
    t = openWindow("", "Print", "resizable");
    i = $afe.jq(n).get(0).outerHTML;
    t.focus();
    dangerou$.setHtml(t.document.body, i);
    window.setTimeout(function(n) {
        n.print();
        n.close()
    }, 10, t)
}
