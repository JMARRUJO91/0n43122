$$WP.Debug.logError = function(n, t) {
    var r, i;
    if (n = n || "",
    "boolean" != typeof t && (t = !0),
    t) {
        try {
            WP && WP.DOM && WP.DOM.Browser.isIE || WP.DOM.Browser.isLegacyEdge ? (console.groupCollapsed($$WP.Debug.messagePrefix + n),
            console.trace(),
