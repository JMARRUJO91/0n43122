    !1
}
;
$$WPUtil.confirmFormSubmission.submitForm = function(n, t, i) {
    var r;
    "function" == typeof t && !1 === t(n, i) || i === $$WP.Components.TOOLBARIDENTIFIER + "continue" && (r = n.onclick,
    n.onclick = null,
    $(n).off(),
    n.click(),
    n.onclick = r)
}
